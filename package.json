{
  "name": "@hebejebe/react-vite-mcp",
  "version": "1.0.3",
  "description": "A CLI tool to scaffold React + Vite + TypeScript projects integrated with Model Context Protocol (MCP).",
  "main": "index.js",
  "bin": {
    "react-vite-mcp": ".build/cli.js"
  },
  "files": [
    ".build/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "prepare": "npm run build",
    "init": "tsx ./src/cli.ts init",
    "feature": "tsx ./src/cli.ts feature",
    "context": "tsx ./src/cli.ts context"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "spa",
    "cli",
    "mcp",
    "scaffolding"
  ],
  "author": "Jonathan Atia",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "links": {
    "npm": "https://www.npmjs.com/package/@hebejebe/react-vite-mcp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CamelJohn/react-vite-mcp.git"
  },
  "homepage": "https://github.com/CamelJohn/react-vite-mcp#readme",
  "bugs": {
    "url": "https://github.com/CamelJohn/react-vite-mcp/issues"
  }
}
